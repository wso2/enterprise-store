<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="ESTestSuite" parallel="false">
    <parameter name="useDefaultListeners" value="false"/>

    <listeners>
        <listener class-name="org.wso2.carbon.automation.engine.testlisteners.TestExecutionListener"/>
        <listener class-name="org.wso2.carbon.automation.engine.testlisteners.TestSuiteListener"/>
        <listener class-name="org.wso2.carbon.automation.engine.testlisteners.TestManagerListener"/>
        <listener class-name="org.wso2.carbon.automation.engine.testlisteners.TestReportListener"/>
        <listener class-name="org.wso2.carbon.automation.engine.testlisteners.TestTransformerListener"/>
    </listeners>


    <test name="ES-UI-extension-tests" parallel="false" verbose="2" preserve-order="true">
        <!--<packages>-->
            <!--<package name="org.wso2.es.ui.integration.extension.test.*"/>-->
        <!--</packages>-->
        <classes>
            <class name="org.wso2.es.ui.integration.extension.test.publisher.ESPublisherAddedAssetTestCase" />
            <class name="org.wso2.es.ui.integration.extension.test.publisher.ESPublisherAssetCaramelPageTestCase" />
            <class name="org.wso2.es.ui.integration.extension.test.publisher.ESPublisherAssetNewPageTestCase" />
            <class name="org.wso2.es.ui.integration.extension.test.publisher.ESPublisherAssetOverrideExistingPageTestCase"/>
            <class name="org.wso2.es.ui.integration.extension.test.publisher.ESPublisherAssetOverridePartialTestCase"/>
            <class name="org.wso2.es.ui.integration.extension.test.publisher.ESPublisherNewGlobalPageTestCase"/>
            <class name="org.wso2.es.ui.integration.extension.test.publisher.ESPublisherOverridenGlobalPageTestCase"/>

            <class name="org.wso2.es.ui.integration.extension.test.store.ESStoreAddedAssetTestCase" />
            <class name="org.wso2.es.ui.integration.extension.test.store.ESStoreAssetNewPageTestCase"/>
            <class name="org.wso2.es.ui.integration.extension.test.store.ESStoreAssetOverrideExistingPageTestCase"/>
            <class name="org.wso2.es.ui.integration.extension.test.store.ESStoreNewGlobalPageTestCase"/>
            <class name="org.wso2.es.ui.integration.extension.test.store.ESStoreOverriddenGlobalPageTestCase"/>
        </classes>
    </test>


</suite>
