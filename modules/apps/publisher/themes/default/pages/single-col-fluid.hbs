<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<head>
    <title>{{include title}}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap -->
    <link href="{{url "/themes/default/css/bootstrap.min.css"}}" rel="stylesheet" media="screen">
    <link href="{{url "/themes/default/css/bootstrap-responsive.min.css"}}" rel="stylesheet">
    <!-- fontawesome -->
    <link rel="stylesheet" href="{{url "/themes/default/css/font-awesome.min.css"}}">

    <!-- publisher styles -->
    <link href="{{url "/themes/default/css/es-publisher-commons.css"}}" rel="stylesheet" media="screen">

    <script src="{{url "/themes/default/js/modernizr.custom.js"}}"></script>
</head>
<body>
{{ include header}}
<div class="container-assets container">
    <div class="row-fluid ribbon">
        {{include ribbon}}
    </div>
    <div class="row-fluid content-area">
        <div class="span3 publisher-left">
            {{include leftnav}}
        </div>
        <div class="span9 publisher-right">
            {{include listassets}}
        </div>
    </div>
    <div class="row-fluid publisher-footer">
        <div class="span12">
            {{>footer}}
        </div>
    </div>
</div>

<script src="{{url "/themes/default/js/jquery-1.10.2.min.js"}}"></script>
<script src="{{url "/themes/default/js/bootstrap.min.js"}}"></script>
<script src="{{url "/themes/default/js/jquery.dlmenu.js"}}"></script>
<script src="{{url "/themes/default/js/publisher.commons.js"}}"></script>
<script src="{{url "/themes/default/js/logic/create.asset.js"}}"></script>
<script>
    $(function() {
        $( '#dl-menu' ).dlmenu({
            animationClasses : { classin : 'dl-animate-in-2', classout : 'dl-animate-out-2' }
        });
    });
</script>
    <script src="{{url "/themes/default/js/raphael-min.js"}}"></script>
    <script src="{{url "/themes/default/js/graph.js"}}"></script>
    <script src="{{url "/themes/default/js/logic/lifecycle.asset.js"}}"></script>
</body>
</html>
