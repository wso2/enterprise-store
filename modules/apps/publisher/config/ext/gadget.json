{
    "applyTo": "gadget",


    "import": [
        "/modules/ext/scripts/rxt.importer.js",
        "/modules/ext/scripts/form.exporter.js"],

    "fieldPropertyRules": [
        "overview.name:order=1",
        "overview.provider:order=2",
        "overview.description:order=3",
        "overview.version:order=4",
        "overview.status:order=5",
	"overview.status:save=lcapi-datasource"

    ],

    "link": [
        "overview.stage:value=*.lifecycleState"
    ],

    "fieldProperties": [

        {
            "field": "*",
            "name": "order",
            "value": ""
        },
        {
            "field": "*",
            "name": "hidden",
            "value": "false"
        },
        {
            "field": "*",
            "name": "visible",
            "value": "true"
        },
	{
	    "field":"overview.*",
	    "name":"save",
	    "value":"default"
	}
    ]
}
